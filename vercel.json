{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000"
            },
            "dest": "/static/$1"
        }
    ]
}